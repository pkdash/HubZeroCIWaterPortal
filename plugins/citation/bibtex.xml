<install version="1.5" type="plugin" group="groups">
	<name>Citation - BibTex</name>
	<author>HUBzero</author>
	<copyright>Copyright 2005-2011 Purdue University. All rights reserved.</copyright>
	<license>http://www.gnu.org/licenses/lgpl-3.0.html LGPLv3</license>
	<description>Imports BibTex data</description>
	<files>
		<filename plugin="bibtex">bibtex.php</filename>
	</files>
	<languages>
		<language tag="en-GB">en-GB.plg_citation_bibtext.ini</language>
	</languages>
	<params>
		<param type="text" name="title_match_percent" label="% Title Match (Import)" description="" size="30" default="90%" />
	</params>
</install>

